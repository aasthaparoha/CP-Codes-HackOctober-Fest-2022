#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
      while(t--){
          int A,B,C;
          cin>>A>>B>>C;
          if(((A+B)%2)!=0){
              cout<<"YES"<<endl;
          }
          else if(((B+C)%2)!=0){
              cout<<"YES"<<endl;
          }
          else if(((C+A)%2)!=0){
              cout<<"YES"<<endl;
          }
          else{
              cout<<"NO"<<endl;
          }
      }
	return 0;
}
